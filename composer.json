{
    "name": "mibo/prices-combinations",
    "type": "library",
    "description": "Extension of mibo/prices lib",
    "keywords": [
        "PHP",
        "Properties",
        "Property",
        "Prices",
        "Price",
        "Calculation",
        "Math",
        "Mathematics",
        "Mathematical",
        "Currencies",
        "Currency",
        "Value",
        "VAT",
        "Tax",
        "Exchange",
        "Exchanging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MiBo\\Prices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Prices\\Tests\\": [
                "tests/Core/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "mibo/prices": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "platform-check": true,
        "lock": false
    }
}
